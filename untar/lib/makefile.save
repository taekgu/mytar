HEAP = heap_Pop
HUFF = huffNode
TREE = tree_loadHuff
WRT = writeData
MAIN = tmain

FUNCS = $(HUFF) $(WRT).o $(TREE).o
LIB = libdecodingfuncs.a
HPWD = /home/rok/mytar
HEAD = $(HPWD)/head.h

$(LIB) : $(HEAD) $(FUNCS)
	ar rv $(LIB) $(FUNCS)

$(HUFF).o : $(HEAD) $(HUFF).cpp
	gcc -c $(HUFF).cpp -I$(HPWD)

$(WRT).o : $(HEAD) $(WRT).cpp
	gcc -c $(WRT).cpp -I$(HPWD)

$(TREE).o : $(HEAD) $(TREE).cpp
	gcc -c $(TREE).cpp -I$(HPWD)

clean : 
	rm $(LIB) $(FUNCS)
